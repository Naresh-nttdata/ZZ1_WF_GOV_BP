{
  "_version": "1.60.0",
  "sap.app": {
    "id": "gwm.coremytask",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:fpm",
      "version": "1.16.5",
      "toolsId": "b1f4a435-8dbb-4423-b1a6-ac40fca6359b"
    },
    "dataSources": {
      "annotation": {
        "type": "ODataAnnotation",
        "uri": "annotations/annotation.xml",
        "settings": {
          "localUri": "annotations/annotation.xml"
        }
      },
      "mainService": {
        "uri": "/sap/opu/odata4/gwm/sb_tasks/srvd/gwm/sd_tasks/0001/",
        "type": "OData",
        "settings": {
          "annotations": [
            "annotation"
          ],
          "localUri": "localService/mainService/metadata.xml",
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.120.14",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.fe.core": {},
        "sap.fe.templates": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "gwm.coremytask.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "@i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "i18n/i18n.properties"
      }
    },
    "resources": {
      "css": []
    },
    "routing": {
      "config": {},
      "routes": [
        {
          "name": "xGWMxCE_TASKSMain",
          "pattern": ":?query:",
          "target": "xGWMxCE_TASKSMain"
        },
        {
          "name": "xGWMxCE_TASKSObjectPage",
          "pattern": "xGWMxCE_TASKS({xGWMxCE_TASKSKey}):?query:",
          "target": "xGWMxCE_TASKSObjectPage"
        }
      ],
      "targets": {
        "xGWMxCE_TASKSMain": {
          "type": "Component",
          "id": "xGWMxCE_TASKSMain",
          "name": "sap.fe.core.fpm",
          "options": {
            "settings": {
              "controlConfiguration":{
                "@com.sap.vocabularies.UI.v1.LineItem":{
                  "columns":{
                    "GWMNav":{
                      "header": "",
                      "template": "gwm.coremytask.ext.view.navBtnColumn",
                      "horizontalAlign": "Center",
                      "width": "auto",
                      "importance": "High",
                      "position": {
                        "placement": "After",
                        "anchor": "DataField::Objectdescription"
                      }
                    }
                  }
                }
              },
              "navigation": {
                "xGWMxCE_TASKS": {
                  "detail": {
                    "route": "xGWMxCE_TASKSObjectPage"
                  }
                }
              },
              "contextPath": "/xGWMxCE_TASKS",
              "viewName": "gwm.coremytask.ext.main.Main"
            }
          }
        },
        "xGWMxCE_TASKSObjectPage": {
          "type": "Component",
          "id": "xGWMxCE_TASKSObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "navigation": {},
              "contextPath": "/xGWMxCE_TASKS"
            }
          }
        }
      }
    }
  },
  "sap.fiori": {}
}
