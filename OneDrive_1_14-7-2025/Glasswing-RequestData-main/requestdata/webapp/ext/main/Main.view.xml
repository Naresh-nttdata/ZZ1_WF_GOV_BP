<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:macros="sap.fe.macros"
    xmlns:html="http://www.w3.org/1999/xhtml" controllerName="gwm.requestdata.ext.main.Main">
    <Page id="Main" title="{i18n>MainTitle}">
        <content>
            <VBox id = "requestDataHeader">
                <macros:FilterBar id="requestDataFilterBar" metaPath="@com.sap.vocabularies.UI.v1.SelectionFields"/>
                <macros:Table id = "requestDataFilterTable" metaPath="@com.sap.vocabularies.UI.v1.LineItem" filterBar="requestDataFilterBar" rowPress=".onRowPress" />
            </VBox>
        </content>
    </Page>
</mvc:View>